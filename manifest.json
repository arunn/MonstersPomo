{
  "manifest_version": 2,

  "name": "pomodoro",
  "description": "This is an attempt at creating a sample pomodoro extension",
  "version": "1.0",
  "background" : {
    "scripts" : ["background.js"]
  },
  "browser_action": {
    "default_icon": "monsters_logo.png",
    "default_title": "Monster's Pomodoro!"
  },
  "permissions": [
    "notifications",
    "http://*/",
    "https://*/",
    "tabs"
  ],
  "web_accessible_resources": [ "james_sullivan_huge.jpg" ]

}